<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplicationdemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_received_doc"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_received_doc"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplicationDemo"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".Medicinebook_details"
            android:exported="false"
            android:parentActivityName=".Medicine_book"
            />
        <activity
            android:name=".Naturalway_show"
            android:exported="false"
            android:parentActivityName=".Medicine_type" />
        <activity
            android:name=".Disease_book"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Docthree"
            android:exported="true"
            android:parentActivityName=".Medicine_show" />
        <activity
            android:name=".Doctwo"
            android:exported="true"
            android:parentActivityName=".Medicine_show" />
        <activity
            android:name=".Docone"
            android:exported="true"
            android:parentActivityName=".Medicine_show" />
        <activity
            android:name=".Contactus"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".About_us"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Search"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Reference"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Medicine_show"
            android:exported="false"
            android:parentActivityName=".Medicine_type" />
        <activity
            android:name=".Medicine_type"
            android:exported="true"
            android:parentActivityName=".Search" />
        <activity
            android:name=".Medicine_book"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>